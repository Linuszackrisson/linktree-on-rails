<div id="<%= dom_id profile %>">
  <p>
    <strong>Name:</strong>
    <%= profile.name %>
  </p>

  <p>
    <%= link_to profile.youtube,  profile.youtube %>
  </p>

  <p>
    <%= link_to profile.twitch,  profile.twitch %>
  </p>

  <p>
    <%= link_to profile.twitter,  profile.twitter %>
  </p>

  <p>
    <%= profile.email %>
  </p>

  <p>
    <%= profile.other %>
  </p>

</div>
